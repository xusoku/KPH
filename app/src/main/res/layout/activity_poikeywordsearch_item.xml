<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/common_measure_75dp"
    android:background="@color/white"
    android:orientation="vertical">
    <com.davis.kangpinhui.views.CustomTypefaceTextView
        android:layout_marginLeft="@dimen/common_measure_15dp"
        android:id="@+id/poi_item_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_measure_5dp"
        android:text="sdfasf"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size_17"/>
    <com.davis.kangpinhui.views.CustomTypefaceTextView
        android:layout_marginLeft="@dimen/common_measure_15dp"
        android:id="@+id/poi_item_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_measure_5dp"
        android:text="sdfasf"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/colorgray"
        android:textSize="@dimen/text_size_11"/>
    <com.davis.kangpinhui.views.CustomTypefaceTextView
        android:layout_marginLeft="@dimen/common_measure_15dp"
        android:id="@+id/poi_item_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_measure_5dp"
        android:text="sdfasf"
        android:textColor="@color/colorgray"
        android:singleLine="true"
        android:ellipsize="end"
        android:textSize="@dimen/text_size_11"/>
</LinearLayout>